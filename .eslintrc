{
  "extends": "./node_modules/eslint-config-airbnb/base.js",
  "env": {
    "node": true
  },
  "rules": {
    "max-len": 0,
    "quote-props": [1, "consistent-as-needed"],
    "no-cond-assign": [2, "except-parens"],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-fallthrough": 0,
    "no-console": 0
  }
}
